{
  "name": "cat-ui",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "tsc": "tsc",
    "styleguidist": "styleguidist",
    "dev": "tsc && styleguidist server",
    "start": "tsc && styleguidist server",
    "min": "react-scripts build",
    "styleguidist_build": "styleguidist build",
    "styleguidist_deploy": "yarn && yarn run styleguidist_build && pm2 reload cat-ui-styleguidist",
    "styleguidist_start": "NODE_ENV=test pm2 start ./server.js --name=cat-ui-styleguidist",
    "styleguidist_stop": "pm2 stop cat-ui-styleguidist",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "lint:fix": "npm run lint -- --fix",
    "prettier:base": "prettier --parser typescript --single-quote --trailing-comma none",
    "prettier:check": "npm run prettier:base -- -l \"src/**/*.{ts,tsx}\"",
    "prettier:write": "npm run prettier:base -- --write \"src/**/*.{ts,tsx}\"",
    "test": "jest",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "lodash": "^4.17.11",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-scripts": "2.1.8"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@types/classnames": "^2.2.7",
    "@types/jest": "^24.0.11",
    "@types/node": "^11.11.4",
    "@types/prop-types": "^15.7.0",
    "@types/react": "^16.8.8",
    "@types/source-map-support": "^0.5.0",
    "babel-loader": "^8.0.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react-app": "^7.0.2",
    "css-loader": "^2.1.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "file-loader": "^3.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.5.0",
    "jest-transform-css": "^2.0.0",
    "jsdom": "^14.0.0",
    "lint-staged": "^8.1.5",
    "node-sass": "^4.11.0",
    "prettier": "^1.16.4",
    "react-docgen-typescript": "^1.12.3",
    "react-styleguidist": "^9.0.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-jest": "^24.0.0",
    "ts-loader": "^5.3.3",
    "tslint": "^5.14.0",
    "typescript": "^3.3.3333",
    "webpack": "^4.29.6"
  }
}
