{
  "name": "@xmly/cat-ui",
  "version": "0.0.105",
  "main": "lib/components/index.js",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git@gitlab.ximalaya.com:HIFE/cat-ui.git"
  },
  "scripts": {
    "tsc": "tsc",
    "styleguidist": "styleguidist",
    "dev": "tsc && styleguidist server",
    "start": "tsc && styleguidist server",
    "release": "./scripts/release.sh",
    "css:build": "./scripts/build-css.sh",
    "new": "./scripts/new-component.sh",
    "build": "tsc --build && yarn run test",
    "test": "jest",
    "update:snapshot": "jest --updateSnapshot",
    "styleguidist:build": "styleguidist build && yarn run test",
    "styleguidist:publish": "yarn && yarn run styleguidist:build && pm2 reload cat-ui-styleguidist",
    "styleguidist:start": "NODE_ENV=test pm2 start ./server.js --name=cat-ui-styleguidist",
    "styleguidist:stop": "pm2 stop cat-ui-styleguidist",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "lint:fix": "npm run lint -- --fix",
    "prettier:base": "prettier --parser typescript --single-quote --trailing-comma none",
    "prettier:check": "npm run prettier:base -- -l \"src/**/*.{ts,tsx}\"",
    "prettier:write": "npm run prettier:base -- --write \"src/**/*.{ts,tsx}\""
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "lodash": "^4.17.11",
    "memoize-one": "^5.0.2",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "styled-components": "^4.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@types/classnames": "^2.2.7",
    "@types/enzyme": "^3.9.1",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.11",
    "@types/lodash": "^4.14.123",
    "@types/memoize-one": "^4.1.1",
    "@types/node": "^11.11.4",
    "@types/prop-types": "^15.7.0",
    "@types/react": "^16.8.8",
    "@types/react-dom": "^16.8.3",
    "@types/react-test-renderer": "^16.8.1",
    "@types/source-map-support": "^0.5.0",
    "@types/styled-components": "^4.1.12",
    "awesome-typescript-loader": "^5.2.1",
    "babel-preset-env": "^1.7.0",
    "css-loader": "^2.1.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "file-loader": "^3.0.1",
    "glob": "^7.1.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.5.0",
    "jest-transform-css": "^2.0.0",
    "jsdom": "^14.0.0",
    "lint-staged": "^8.1.5",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "prettier": "^1.16.4",
    "react-docgen-typescript": "^1.12.3",
    "react-styleguidist": "^9.0.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-jest": "^24.0.0",
    "ts-loader": "^5.3.3",
    "tslint": "^5.14.0",
    "typescript": "^3.3.3333",
    "typescript-plugin-styled-components": "^1.4.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0"
  }
}
