{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "lib": ["es5", "es6", "es7", "dom"],
    "noImplicitAny": true,
    "removeComments": true,
    "noImplicitReturns": false,
    "noImplicitThis": false,
    "sourceMap": true,
    "skipLibCheck": true,
    "outDir": "lib",
    "jsx": "react"
  },
  "exclude": ["node_modules", "lib", "styleguide"]
}
